<h1 align="center">Listing All Categories</h1>


<div class="center">
  <%= will_paginate @category %>
</div>


<div align="center">

  <% @category.each do |category| %>
    <div class="col-md-4 col-md-offset-4">
      <div class="well">
        <div class="movie-title">
          <%= link_to "#{category.name}", category_path(category) %>
        </div>
      </div>
    </div>
  <%end %>
</div>


